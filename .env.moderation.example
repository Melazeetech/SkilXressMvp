# Video Moderation API Keys
# Add these to your .env file

# OpenAI API Key (for skill classification and video analysis)
OPENAI_API_KEY=your_openai_api_key_here

# Google Cloud Video Intelligence API (for content moderation)
# Alternative: Use a service account JSON key
GOOGLE_CLOUD_PROJECT_ID=your_project_id
GOOGLE_APPLICATION_CREDENTIALS=./path/to/service-account-key.json

# Or use Cloudinary AI Moderation (alternative)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# Moderation thresholds (0-1 scale)
MODERATION_NUDITY_THRESHOLD=0.7
MODERATION_VIOLENCE_THRESHOLD=0.7
MODERATION_SKILL_CONFIDENCE_THRESHOLD=0.6

# Supabase Service Role Key (for backend operations)
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
